<?xml version='1.0' encoding='utf-8'?>
<widget id="com.appgenny.latest" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Appgenny</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="15" />
    <preference name="android-maxSdkVersion" value="28" />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="postRows" value="3" />
    <preference name="onesignal" value="58cf1e0f-765a-426e-a07a-9c4735ce8b05" />
    <preference name="admobBannerId" value="ca-app-pub-2712145761453335/8490260154" />
    <preference name="admobInterstitialId" value="ca-app-pub-2712145761453335/5480953432" />
    <preference name="admobAppId" value="ca-app-pub-2712145761453335~7694400848" />
    <preference name="SplashScreenDelay" value="4000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="FadeSplashScreenDuration" value="400" />
    <preference name="SplashScreenBackgroundColor" value="white" />
    <preference name="loadUrlTimeoutValue" value="70000" />
    <engine name="android" spec="^8.0.0" />
    <plugin name="com.bunkerpalace.cordova.YoutubeVideoPlayer" spec="https://github.com/JonSmart/CordovaYoutubeVideoPlayer.git" />
    <plugin name="com.santinowu.cordova.imagedownloader" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-android-play-services-gradle-release" spec="^4.0.0">
        <variable name="PLAY-SERVICES-LOCATION" value="17.0.0" />
    </plugin>
    <plugin name="cordova-plugin-admobpro" spec="^2.49.0">
        <variable name="PLAY_SERVICES_VERSION" value="17.0.0" />
        <variable name="ADMOB_APP_ID" value="ca-app-pub-3756352465808334~2148080951" />
    </plugin>
    <plugin name="cordova-plugin-app-version" spec="^0.1.9" />
    <plugin name="cordova-plugin-customconfigparameters" spec="^3.0.0" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.2" />
    <plugin name="cordova-plugin-screen-orientation" spec="^3.0.2" />
    <plugin name="cordova-plugin-shortcuts-android" spec="^0.1.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.3" />
    <plugin name="onesignal-cordova-plugin" spec="^2.6.0" />
</widget>